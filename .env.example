# AB-Performance Server Konfiguration
# Kopieren Sie diese Datei zu .env und passen Sie die Werte an

# Server-Einstellungen
PORT=3000

# E-Mail-Konfiguration
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=your-email@example.com
EMAIL_PASS=your-password
EMAIL_FROM=website@ab-performance.rocks
EMAIL_TO=info@ab-performance.rocks

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-api-key

# MongoDB-Verbindung (für zukünftige Funktionen)
MONGODB_URI=mongodb://localhost:27017/ab-performance 